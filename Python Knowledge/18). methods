#creare diff diff methods and use as object

class student:
    college_name = "abc college"
    
    def __init__(self, name, marks):
        self.name = name
        self.marks = marks
    
    def welcome(self):
        print("Welcome student", self.name)
        
    def get_marks(self):
        return self.marks

# Creating an instance of student
s1 = student("Karan", 99)
s1.welcome()    # Output: Welcome student Karan
print(s1.get_marks())
