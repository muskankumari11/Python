#constructor

class student():
    name="ram"
    
    #deffault constructor
    def __init__(self):
        pass
    #parameterised constructor
    def __init__(self,name,marks):
        self.name=name
        self.marks=marks
        print("adding new student in db")
        #object create
s1=student("karan",44)
print(s1.name,s1.marks)

s2=student("arjun",77)
print(s2.name,s2.marks)
