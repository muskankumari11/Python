#private
"""
class Account:
    def __init__(self, acc_num, acc_pass):
        self.acc_num = acc_num
        self.__acc_pass = acc_pass

    def reset_pass(self):
        print(self.__acc_pass)


# Creating an instance of Account
acc1 = Account("12345", "abcde")

# Accessing public attribute
print(acc1.acc_num)

# Accessing private attribute using a getter method
print(acc1.reset_pass())
"""

#another example
class Person:
    __name = "anonymous"
    
    def __hello(self):
        print("Hello person")
    
    def welcome(self):
        self.__hello()

p1 = Person()
p1.welcome()  # This will print "Hello person"
